{
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.1",
    "@mui/material": "^6.4.1",
    "@types/react-beautiful-dnd": "^13.1.8",
    "colorthief": "^2.4.0",
    "concurrently": "^9.1.2",
    "next": "^15.1.6",
